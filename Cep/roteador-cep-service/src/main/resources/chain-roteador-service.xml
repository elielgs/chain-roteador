<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
          http://www.springframework.org/schema/beans 
          http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="Chain" class="roteador.chain.Chain">
		<constructor-arg>
			<list>
				<value>service/chain/processo/xml/processSalvarOrganizacoes.xml</value>
				<value>service/chain/processo/xml/processFindAPIsByUser.xml</value>
				<value>service/chain/processo/xml/processFindAPIVersionById.xml</value>
				<value>service/chain/processo/xml/processFindDocByAPIVersion.xml</value>
			</list>
		</constructor-arg>
		<!-- <property name="transactionManager" ref="txManagerCep"/> -->
	</bean>

	<bean id="Pipeline" class="roteador.chain.pipeline.Pipeline" />

</beans>